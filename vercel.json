{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "installCommand": "npm install",
    "framework": null,
    "functions": {
        "api/index.ts": {
            "maxDuration": 30,
            "memory": 1024,
            "includeFiles": "{api/dist/**,dist/**}"
        }
    },
    "rewrites": [{
        "source": "/(.*)",
        "destination": "/api"
    }]
}